<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="A1" target="SYSTEM_OUT" follow="true">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS } %-6p %m  [%t] [%F:%L]%n"/> 
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="A1"/>
        </Root>
		<logger name="com.sshtools.j2ssh" level="OFF"> </logger>
		<logger name="se.ericsson.jcat.fw.utils.Ssh2Session" level="INFO"> </logger>
		<logger name="se.ericsson.jcat.fw.utils.Ssh2SessionCore" level="INFO"> </logger>
    </Loggers>
</Configuration>
